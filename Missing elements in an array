#include <stdio.h>
void findMissing(int arr[], int n)
{
    int total = n * (n + 1) / 2; 
    int sum = 0;
    for (int i = 0; i < n - 1; i++)
   {
        sum += arr[i]; 
    }
    int missing = total - sum; 
    printf("The missing element is: %d\n", missing);
}
int main()
{
    int arr[] = {1, 2, 4, 5, 6}; 
    int n = sizeof(arr) / sizeof(arr[0]) + 1; 
    findMissing(arr, n);
    return 0;
}
